{
    "name": "CloudCore - AWS S3 File Manager",
    "short_name": "CloudCore",
    "description": "Free, open-source AWS S3 file manager. Upload, download, organize, and share files directly from your browser with no backend required. Enterprise-grade security with your own AWS credentials.",
    "start_url": "/",
    "scope": "/",
    "id": "cloudcore-s3-manager",
    "display": "standalone",
    "display_override": [
        "window-controls-overlay",
        "standalone",
        "browser"
    ],
    "background_color": "#000000",
    "theme_color": "#9333ea",
    "orientation": "any",
    "lang": "en-US",
    "dir": "ltr",
    "icons": [
        {
            "src": "/icon-192.svg",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "any"
        },
        {
            "src": "/icon-192.svg",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "maskable"
        },
        {
            "src": "/icon-512.svg",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "any"
        },
        {
            "src": "/icon-512.svg",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "maskable"
        }
    ],
    "categories": [
        "productivity",
        "utilities",
        "developer",
        "business"
    ],
    "screenshots": [
        {
            "src": "/og-image.png",
            "sizes": "1200x630",
            "type": "image/png",
            "form_factor": "wide",
            "label": "CloudCore main interface showing file explorer with drag-and-drop capabilities"
        }
    ],
    "shortcuts": [
        {
            "name": "Upload Files",
            "short_name": "Upload",
            "description": "Upload files to your S3 bucket",
            "url": "/?action=upload",
            "icons": [
                {
                    "src": "/icon-192.svg",
                    "sizes": "192x192"
                }
            ]
        },
        {
            "name": "Create Folder",
            "short_name": "New Folder",
            "description": "Create a new folder in your S3 bucket",
            "url": "/?action=newfolder",
            "icons": [
                {
                    "src": "/icon-192.svg",
                    "sizes": "192x192"
                }
            ]
        }
    ],
    "related_applications": [],
    "prefer_related_applications": false,
    "handle_links": "preferred",
    "launch_handler": {
        "client_mode": "navigate-existing"
    }
}