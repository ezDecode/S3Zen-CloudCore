{
    "name": "cloudcore-backend",
    "version": "2.0.0",
    "description": "CloudCore backend - S3 file manager with multi-bucket support and Supabase auth",
    "main": "src/server.js",
    "scripts": {
        "start": "node src/server.js",
        "dev": "node --watch src/server.js",
        "build": "node scripts/build.js",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "db:migrate": "echo 'Run migrations via Supabase CLI: npx supabase db push'",
        "db:reset": "echo 'Reset database via Supabase CLI: npx supabase db reset'",
        "generate:key": "node -e \"console.log(require('crypto').randomBytes(32).toString('hex'))\""
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.693.0",
        "@aws-sdk/client-sts": "^3.693.0",
        "@aws-sdk/s3-request-presigner": "^3.693.0",
        "@supabase/supabase-js": "^2.46.2",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.21.1",
        "express-rate-limit": "^7.4.1",
        "helmet": "^8.0.0",
        "jose": "^5.9.6",
        "multer": "^1.4.5-lts.1",
        "nanoid": "^3.3.7",
        "sharp": "^0.33.5",
        "sqlite3": "^5.1.7",
        "uuid": "^11.0.3",
        "zod": "^4.3.5"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@vitest/coverage-v8": "^4.0.14",
        "@vitest/ui": "^4.0.14",
        "fast-check": "^3.23.1",
        "jsdom": "^27.2.0",
        "supertest": "^7.1.4",
        "vitest": "^4.0.14"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
